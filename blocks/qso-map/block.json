{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "m0lxx-qsomap/qsomap",
    "title": "QSO Map",
    "description": "An ADIF log to QSO map tool by M0LXX",
    "icon": "location-alt",
    "category": "common",
    "keywords": [
        "amateur",
        "radio",
        "qso",
        "map",
        "mapping"
    ],

    "supports": {
        "html": false,
        "reusable": false,
        "align": false
    },

    "attributes": { 
        "logs": {
            "type": "array",
            "default": []
        },
        "myCall": {
            "type": "string"
        },
        "qthGrid": {
          "type": "string"
        },
        "originalQthLatitude": {
            "type": "string",
            "default": 0
        },
        "originalQthLongitude": {
            "type": "string",
            "default": 0
        },
        "qthLatitude": {
            "type": "string",
            "default": 0
        },
        "qthLongitude": {
            "type": "string",
            "default": 0
        },
        "furthestQSODistance": {
            "type": "number",
            "default": 0
        },
        "furthestQSOStation": {
            "type": "string",
            "default": ""
        },
        "qsosPerMinute": {
            "type": "number",
            "default": 0
        },
        "uploading": {
            "type": "boolean",
            "default": false
        },
        "numberOfGridsquares": {
            "type": "number",
            "default": 0
        },
        "showHeatmap": {
            "type": "boolean"
        },
        "showStatistics": {
            "type": "boolean"
        },
        "showLines": {
            "type": "boolean"
        },
        "showLog": {
            "type": "boolean"
        }
    },
    "render": "file:./index.php"
}